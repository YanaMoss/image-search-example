{{#each this}}
<a href="{{webformatURL}}" class="photo-card">
   <img src="{{previewURL}}" alt="{{tags}}" loading="lazy" width="{{prewieWidth}}" height="{{prewieHeight}}" class = "img" />
   <div class="info">
     <p class="info-item">
         <b>Likes</b>
         {{likes}}
      </p>
      <p class="info-item">
         <b>Views</b>
         {{views}}
      </p>
      <p class="info-item">
         <b>Comments</b>
         {{comments}}
      </p>
      <p class="info-item">
         <b>Downloads</b>
         {{downloads}}
      </p>
   </div>
</div>
</a>
{{/each}}